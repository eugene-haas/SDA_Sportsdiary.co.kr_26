  <title>A5 landscape</title>


<style type="text/css" media="print">

@page {
  size: landscape;
}

.landscape { 
    width: 100%; 
    height: 100%; 
    margin: 0% 0% 0% 0%; filter: progid:DXImageTransform.Microsoft.BasicImage(Rotation=1); 
} 
</style>

</head>

<!-- Set "A5", "A4" or "A3" for class name -->
